{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "outDir": "./dist",
    "strict": false,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "baseUrl": "./src",
    "paths": {
      "@config/*": ["config/*"],
      "@routes/*": ["routes/*"],
      "@modules/*": ["modules/*"],
      "@models/*": ["models/*"]
    },
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true
  }
}
